<div class="container">
  <div class="cards">
    <% @cocktails.each do |cocktail| %>
      <div class="card" style="background-image: url('<%= cl_image_path cocktail.photo.key, height: 300, width: 400, crop: :fill %>')">
        <%= link_to "#{cocktail.name}", cocktail_path(cocktail) , class: "card-text" %>
      </div>
    <% end %>
  </div>
</div>


